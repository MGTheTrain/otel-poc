{
  "name": "Go OpenTelemetry Service",
  "image": "mcr.microsoft.com/devcontainers/go:1.25-bookworm",
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go"
      ]
    }
  },
  "forwardPorts": [8080],
  "remoteUser": "vscode",
  "remoteEnv": {
    "OTEL_EXPORTER_OTLP_ENDPOINT": "http://localhost:4317"
  }
}